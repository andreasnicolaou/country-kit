{
  "name": "@andreasnicolaou/country-kit",
  "version": "1.0.0",
  "description": "A comprehensive package for working with country data. Includes ISO codes, names in multiple languages, capitals, currencies, continents and more.",
  "main": "dist/index.js",
  "private": false,
  "types": "dist/types/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreasnicolaou/country-kit.git"
  },
  "bugs": {
    "url": "https://github.com/andreasnicolaou/country-kit/issues"
  },
  "author": "Andreas Nicolaou <anicolaou66@gmail.com>",
  "license": "MIT",
  "keywords": [
    "country data",
    "ISO codes",
    "countries",
    "geography",
    "internationalization",
    "translations",
    "TypeScript",
    "country information",
    "geolocation",
    "multi-language support"
  ],
  "jest": {
    "transform": {
      "^.+\\.(ts)$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "prettier": "3.5.3",
    "terser": "^5.39.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
